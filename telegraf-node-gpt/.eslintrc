{
  "env": {
    "es2021": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:@typescript-eslint/recommended-type-checked"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": true
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
  "@typescript-eslint/no-floating-promises": "warn"
}
}
